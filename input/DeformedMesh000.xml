  <ReferenceEvaluator name="deformed_mesh.coordinates" evaluator="deformed_mesh.coordinates.template">
   <Variables>
     <Variable name="deformed_mesh.mesh.variable" />
   </Variables>
   <Bindings>
    <Bind variable="deformed_mesh.nodal_component_dofs" source="deformed_mesh.node.coordinates" />
   </Bindings>
  </ReferenceEvaluator>
  </Region>
</Fieldml>
