     <Variable name="deformed_mesh.mesh.variable" />
   </Variables>
   <SemidenseData data="element.points">
    <DenseIndexes>
     <IndexEvaluator evaluator="library.localNodes.2d.triangle2x2.variable" />
     <IndexEvaluator evaluator="deformed_mesh.mesh.variable.elements" />
    </DenseIndexes>
